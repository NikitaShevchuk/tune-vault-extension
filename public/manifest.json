{
  "manifest_version": 3,
  "name": "Tune Vault",
  "version": "0.0.1",
  "description": "Effortlessly play music on your favorite Discord server!",
  "icons": {
    "16": "./logo-16.png",
    "32": "./logo-32.png",
    "48": "./logo-48.png",
    "128": "./logo-128.png"
  },
  "action": {
    "default_popup": "../index.html"
  },
  "permissions": [ "tabs" ],
  "host_permissions": [
    "http://localhost:5001/"
  ]
}
